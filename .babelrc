{
  "presets": [
    "es2015",
    "react-app",
    "stage-0"
  ],
  "plugins": [
    ["import", { "libraryName": "antd","style":"css"}], // `style: true`会加载 less 文件 style: 'css'
    ["transform-decorators-legacy"],//@高阶组件
    ["transform-class-properties"],//箭头函数绑定 this
    ["transform-runtime"]//避免资源重复加载
//    ["react-hot-loader/babel"]//热加载
  ]
}
